# Environment
ENVIRONMENT=dev
PROJECT_NAME=BioPilot

# Database (PostgreSQL)
POSTGRES_SERVER=db
POSTGRES_USER=biouser
POSTGRES_PASSWORD=biopassword
POSTGRES_DB=biopilot
DATABASE_URL=postgresql://biouser:biopassword@db:5432/biopilot

# Security
SECRET_KEY=change_this_to_a_secure_random_string
ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=30

# MinIO (S3 Compatible)
MINIO_ENDPOINT=minio:9000
MINIO_ROOT_USER=minioadmin
MINIO_ROOT_PASSWORD=minioadmin
# On local dev, bucket name usually needs manual creation or initialization script
S3_BUCKET_NAME=biopilot-files

# Redis
REDIS_URL=redis://redis:6379/0

# Qdrant (Vector DB)
QDRANT_URL=http://qdrant:6333

# AI (Optional for now)
OPENAI_API_KEY=